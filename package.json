{
  "name": "color-assets",
  "scope": "ape",
  "version": "0.0.2",
  "description": "Colors, Gradients, etc. as assets so they can be more easily managed globally.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beatthat/color-assets.git"
  },
  "files": [
    "index.js",
    "scripts",
    "src"
  ],
  "author": "larrykirschner@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beatthat/color-assets/issues"
  },
  "homepage": "https://github.com/beatthat/color-assets",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "install:test": "rm -f *.tgz && npm pack && cd test && npm run clean && npm install",
    "clean": "rm -f *.tgz && cd test && npm run clean",
    "sync:test2src": "rsync -rv --include '*/' --include '*.cs' --exclude '*' --prune-empty-dirs test/Assets/Plugins/${npm_package_scope}/${npm_package_name}/ ./src/${npm_package_name}/"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "property-interfaces": "github:beatthat/property-interfaces#semver:^0.0.1"
  }
}
